{
    "plugins": [
        "prettier"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 7
    },
    "rules": {
        "linebreak-style": 0
    },
    "ignorePatterns": [
        ".prettierrc"
    ]
}
